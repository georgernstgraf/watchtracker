{
    "tasks": {
        "bastel": "deno run --inspect bastel.ts",
        "p:generate": "deno -A prisma generate",
        "p:m": "deno -A prisma migrate",
        "start": "deno run --env-file --allow-env --allow-read --allow-ffi --allow-net bin/app.ts",
        "dev": "node --watch --inspect-brk bin/app.js",
        "purgecss": "purgecss --config purgecss.config.js"
    },
    "imports": {
        "@prisma/client": "npm:@prisma/client@^6.16.3",
        "generated-prisma-client": "./prisma/client/client.ts",
        "@std/assert": "jsr:@std/assert@1",
        "body-parser": "npm:body-parser@^2.2.0",
        "connect-memcached": "npm:connect-memcached@^2.0.0",
        "express": "npm:express@^5.1.0",
        "express-handlebars": "npm:express-handlebars@^8.0.3",
        "express-session": "npm:express-session@^1.18.2",
        "express-static-gzip": "npm:express-static-gzip@^3.0.0",
        "handlebars": "npm:handlebars@^4.7.8",
        "http-errors": "npm:http-errors@^2.0.0",
        "luxon": "npm:luxon@^3.5.0",
        "moment-timezone": "npm:moment-timezone@^0.5.46",
        "ms": "npm:ms@^2.1.3",
        "prisma": "npm:prisma@^6.16.3"
    },
    "dependencies": {
        "@prisma/client": "^5.21.1",
        "npm-check-updates": "^17.1.10",
        "prettier": "^3.3.3",
        "prettier-plugin-ejs": "^1.0.3",
        "purgecss": "^6.0.0",
        "connect-memcached": "^2.0.0",
        "express": "^4.21.1",
        "express-handlebars": "^8.0.1",
        "express-session": "^1.18.1",
        "express-static": "^1.2.6",
        "express-static-gzip": "^2.1.8",
        "handlebars": "^4.7.8",
        "luxon": "^3.5.0",
        "moment-timezone": "^0.5.46",
        "ms": "^2.1.3",
        "prisma": "^6.16.3"
    },
    "fmt": {
        "lineWidth": 150,
        "indentWidth": 4,
        "semiColons": true,
        "singleQuote": false
    }
}
